<div class='border-bottom'></div>

<div class='vendor-header'>Add New Venue</div>

<div class="form-container">
    <form action="/venue" method="POST">
        <div class="form-wrapper">
            <label>Venue</label>
            <input type="text" name="name" />

            <label>Where are you located</label>
            <input type="text" name="location" />

            <label>How many square feet</label>
            <input type="number" name="squareFeet" />

            <label>Is parking space available</label>
            <input type="boolean" name="parkingAvailable" />

            <label>Price</label>
            <input type="number" name="price" />

            <label>Tell us about your venue</label>
            <textarea type="text" name="about" rows="5" cols="20"></textarea>

            <label>Image URL</label>
            <input type="text" name="image" />
        </div>
        <input type="submit" value="Create" />

    </form>
</div>
